---
layout: base
---

<img class="blog-detail-jumbotron" src="../../images/{{ image }}" alt="blog image">

<section class="h-layout">
    <h1>{{ title }}</h1>
    <p>{{ created }}</p>

    {{content | safe}}

    <ul>
        {% for author in allAuthors %}
            <li>
                {{ author }}
            </li>
        {% endfor %}
    </ul>
</section>
